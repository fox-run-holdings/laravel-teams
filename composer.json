{
    "name": "fox-run-holdings/laravel-teams",
    "description": "A Laravel package for team management with invitations and role-based access control",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fox Run Holdings",
            "email": "info@foxrunholdings.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "livewire/livewire": "^3.0",
        "flux-ui/flux-ui-pro": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "FoxRunHoldings\\LaravelTeams\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FoxRunHoldings\\LaravelTeams\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FoxRunHoldings\\LaravelTeams\\Providers\\TeamsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 